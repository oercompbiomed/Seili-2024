   "source": [
    "# classical GSEA plots\n",
    "gseaplot(gsea_results, by = \"all\", title = gsea_results$Description[1], geneSetID = 1)\n",
    "#gseaplot(gsea_results, by = \"all\", title = gsea_results$Description[2], geneSetID = 2)"
